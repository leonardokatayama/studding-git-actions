{
  "clearMocks": true,
  "coverageDirectory": "<rootDir>/coverage",
  "coverageProvider": "v8",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/src/main/configs/dotenv-init.ts"
  ],
  "roots": [
    "<rootDir>/src"
  ],
  "modulePaths": ["<rootDir>"],
  "transform": {
    "^.+\\.(t|j)sx?$": "@swc/jest"
  }
}
